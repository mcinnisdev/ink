{
  "name": "ink-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Ink â€” Markdown-native website starter built on Eleventy + Tailwind CSS",
  "scripts": {
    "dev": "concurrently \"tailwindcss -i src/css/tailwind.css -o src/css/style.css --watch\" \"npx @11ty/eleventy --serve --watch\"",
    "build": "tailwindcss -i src/css/tailwind.css -o src/css/style.css --minify && npx @11ty/eleventy",
    "clean": "rm -rf _site"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@tailwindcss/typography": "^0.5.15",
    "concurrently": "^9.1.2",
    "tailwindcss": "^3.4.17"
  }
}
